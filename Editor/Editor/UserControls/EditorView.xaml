<UserControl x:Class="Editor.UserControls.EditorView"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Editor" 
      xmlns:viewmodels="clr-namespace:Editor.ViewModels"
      mc:Ignorable="d" 
      DataContext="{Binding EditorViewModel, Source={StaticResource ViewModelLocator}}"
      d:DesignHeight="450" d:DesignWidth="800">

    <DockPanel LastChildFill="True" x:Name="RootDock">
    </DockPanel>
</UserControl>
